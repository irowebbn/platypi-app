{
  "name": "platypus",
  "version": "0.13.3",
  "description": "A front-end MVVM framework written in TypeScript",
  "main": "dist/platypus.js",
  "devDependencies": {
    "glob": "latest",
    "grunt": "latest",
    "grunt-cli": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-copy": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-ts": "latest",
    "grunt-ts-bundle": "latest",
    "grunt-tslint": "latest",
    "http-server": "latest",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-cli": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-html-reporter": "latest",
    "karma-ie-launcher": "latest",
    "karma-jasmine": "latest",
    "load-grunt-tasks": "latest",
    "lodash": "latest",
    "rimraf": "latest",
    "tsd": "^0.6.0-beta.5",
    "typescript": "latest"
  },
  "scripts": {
    "prepublish": "npm run tsd && npm run deploy",
    "build-all": "npm run build-examples && npm run build-test && npm run build-src",
    "build-examples": "tsc -p build/tsc/examples",
    "build-src": "tsc -p build/tsc/src",
    "build-test": "tsc -p build/tsc/test",
    "clean-all": "npm run clean-examples && npm run clean-test && npm run clean-src",
    "clean-examples": "rimraf -p examples/app.*",
    "clean-src": "rimraf -p dist/*",
    "clean-test": "rimraf -p src/**/*.js test/**/*.js",
    "deploy": "grunt",
    "examples": "npm run build-examples && npm run server-examples",
    "karma": "karma start karma.conf.js",
    "server-examples": "http-server -p 3000 examples",
    "test": "npm run build-test && npm run karma",
    "tsd": "tsd update -so --config tsd.test.json",
    "watch-examples": "tsc -p build/tsc/examples/watch",
    "watch-src": "tsc -p build/tsc/src/watch",
    "watch-test": "tsc -p build/tsc/test/watch"
  },
